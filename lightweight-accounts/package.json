    "@ownclouders/prettier-config": "0.0.1",
    "@ownclouders/tsconfig": "0.0.6",
    "@types/node": "^22.9.1",
    "eslint": "9.15.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vue-tsc": "2.1.10"
  },
  "peerDependencies": {
    "@ownclouders/web-client": "0.0.1",
    "@ownclouders/web-pkg": "0.0.5-alpha.9",
    "vue": "3.3.4",
    "vue-router": "4.2.0",
    "vue3-gettext": "^2.3.3",
    "vuex": "4.1.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system*"
      ]
    }
  },
  "dependencies": {
    "@ownclouders/web-client": "11.0.4",
