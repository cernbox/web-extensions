{
  "name": "ndmspc-reader",
  "version": "2.0.1",
  "description": "Simple notebook opener",
  "license": "AGPL-3.0",
  "author": "CERNBox",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/cernbox/web-extensions",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts,vue}' --color",
    "build": "pnpm vite build",
    "build:w": "pnpm vite build --watch --mode development"
